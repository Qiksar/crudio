{
  "name": "@qiksar/crudio",
  "version": "0.4.1",
  "description": "Rapidly create rich meaningful test data to accelerate your prototyping and testing processes",
  "bin": "lib/crudio.js",
  "main": "lib/crudio.js",
  "typings": "lib/src/index.d.ts",
  "repository": "github.com:/qiksar/crudio",
  "author": "Chris Nurse",
  "license": "GPL-3.0-only",
  "scripts": {
    "test": "jest --env=node --colors --coverage test --silent=false",
    "build": "tsc -p tsconfig.json",
    "start_cli": "node lib/crudio.js repo/repo.json",
    "up": "docker-compose up -d",
    "down": "docker-compose down",
    "remove": "docker-compose down --rmi all"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/luxon": "^1.15.2",
    "@types/node": "^16.7.10",
    "@types/yaml": "^1.0.2",
    "jest": "^27.1.0",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.8.2",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "axios": "^0.21.3",
    "flatted": "^3.2.6",
    "global": "^4.4.0",
    "jquery-csv": "^1.0.5",
    "luxon": "^1.19.3",
    "yaml": "^1.6.0"
  },
  "keywords": [
    "hasura",
    "postgres",
    "test_data_generation",
    "rapid_prototyping"
  ]
}

{
  "name": "@qiksar/crudio",
  "version": "0.10.0",
  "description": "Rapidly create rich meaningful test data to accelerate your prototyping and testing processes",
  "bin": "lib/crudio_cli.js",
  "main": "lib/crudio_cli.js",
  "typings": "lib/src/index.d.ts",
  "repository": "github.com:/qiksar/crudio",
  "author": "Chris Nurse",
  "license": "GPL-3.0-only",
  "scripts": {
    "test": "jest --env=node --colors --coverage test --silent=false",
    "build": "tsc -p tsconfig.json",
    "start_cli": "node lib/crudio_cli.js -v -w -m datamodel/datamodel.json -i repo/iot.json -d test/unit/output.repo.md",
    "cli_init": "node lib/crudio_cli.js -v -p test/unit/output/test_cli_folder",
    "up": "docker-compose up -d",
    "down": "docker-compose down",
    "remove": "docker-compose down --rmi local"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/luxon": "^1.15.2",
    "@types/node": "^16.7.10",
    "jest": "^27.1.0",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.8.2",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "axios": "^0.21.3",
    "commander-ts": "^0.2.0",
    "flatted": "^3.2.6",
    "global": "^4.4.0",
    "jsonschema": "^1.4.1",
    "luxon": "^1.19.3"
  },
  "keywords": [
    "hasura",
    "postgres",
    "testing",
    "lowcode",
    "test_data_generation",
    "rapid_prototyping"
  ]
}

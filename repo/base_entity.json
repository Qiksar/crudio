{
	"$schema": "https://raw.githubusercontent.com/Qiksar/crudio/main/schema/crudio.json",
	"entities": {
		"Entity": {
			"abstract": true,
			"snippets": [
				"id"
			],
			"fields": {
				"created": {
					"required": true,
					"type": "timestamp",
					"name": "created",
					"generator": "[timestamp]"
				},
				"deleted": {
					"type": "timestamp",
					"name": "deleted"
				}
			}
		},
		"Lookup": {
			"abstract": true,
			"inherits": "Entity",
			"fields": {
				"name": {
					"required": true,
					"type": "string",
					"unique": true
				},
				"value": {
					"type": "string"
				}
			}
		}
	}
}
{
	"entities": {
		"Entity": {
			"abstract": true,
			"id": {
				"type": "uuid",
				"name": "id",
				"key": true,
				"generator": "[uuid]"
			},
			"created": {
				"type": "timestamp",
				"name": "created",
				"generator": "[timestamp]"
			},
			"deleted": {
				"type": "timestamp",
				"name": "deleted"
			}
		},
		"Organisation": {
			"inherits": "Entity",
			"count": 4,
			"name": {
				"type": "string",
				"name": "name",
				"generator": "[organisation_name]"
			},
			"address": {
				"type": "string",
				"name": "address",
				"generator": "[address]"
			},
			"email": {
				"type": "string",
				"name": "email",
				"generator": "contact@[!name].org.au"
			}
		},
		"Person": {
			"abstract": true,
			"inherits": "Entity",
			"snippets":["firstname","lastname","address","email"],
			"dob": {
				"type": "date",
				"name": "dob",
				"generator": "[dob]"
			},
			"height": {
				"type": "integer",
				"name": "height",
				"generator": "[height]"
			},
			"weight": {
				"type": "integer",
				"name": "weight",
				"generator": "[weight]"
			}
		},
		"User": {
			"count": 200,
			"inherits": "Person",
			"email": {
				"unique": true,
				"generator": "[user_email]"
			},
			"relationships": [
				{
					"type": "one",
					"to": "Organisation"
				}
			]
		},
		"Program": {
			"inherits": "Entity",
			"name": {
				"type": "string",
				"name": "name",
				"generator": "[program_name]"
			},
			"mission": {
				"type": "string",
				"name": "mission",
				"generator": "[program_mission]"
			},
			"relationships": [
				{
					"type": "one",
					"to": "Organisation"
				}
			]
		},
		"Cactus": {
			"inherits": "Entity",
			"height": {
				"type": "integer",
				"name": "height",
				"generator": "[length]"
			}
		},
		"Cohort": {
			"inherits": "Entity",
			"name": {
				"type": "string",
				"name": "name",
				"generator": "[cohort_name]"
			},
			"relationships": [
				{
					"type": "one",
					"to": "Program"
				}
			]
		},
		"Survey": {
			"inherits": "Entity",
			"name": {
				"type": "string",
				"name": "name",
				"generator": "[survey_name]"
			},
			"relationships": [
				{
					"type": "one",
					"to": "Program"
				}
			]
		},
		"Client": {
			"inherits": "Person",
			"relationships": [
				{
					"type": "one",
					"to": "Cohort"
				}
			]
		},
		"Tag": {
			"inherits": "Entity",
			"count": 25,
			"name": {
				"unique": "true",
				"type": "string",
				"name": "name",
				"generator": "[tag]"
			}
		},
		"Blog": {
			"inherits": "Entity",
			"article_text": {
				"type": "string",
				"name": "article",
				"generator": "[article]"
			},
			"published_date": {
				"type": "timestamp",
				"name": "published_date",
				"generator": "[timestamp]"
			},
			"relationships": [
				{
					"type": "many",
					"to": "Tag",
					"name": "BlogTags",
					"count": 2
				},
				{
					"type": "one",
					"to": "User",
					"name": "Author"
				}
			]
		}
	}
}